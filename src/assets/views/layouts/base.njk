<!DOCTYPE html>
<html lang="{{ lang or 'en' }}" dir="{{ dir or 'ltr' }}">
  <head>
    {#- Stylesheet -#}
    <link rel="stylesheet" href="{{ '/assets/styles/styles.css' | url }}" />

    {# Fonts #}
    <style>
      /* Import /assets/fonts/fontsource-variable/merriweather-latin-standard-normal.woff2 */
      @font-face {
        font-family: "Fira Sans";
        font-style: normal;
        font-weight: 700;
        font-display: swap;
        src: url(/assets/fonts/va9B4kDNxMZdWfMOD5VnLK3eRhf6Xl7Glw.woff2) format("woff2");
        unicode-range:
          U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2122,
          U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
      }

      @font-face {
        font-family: "Merriweather";
        font-style: normal;
        font-weight: 400;
        font-display: swap;
        src: url(/assets/fonts/u-440qyriQwlOrhSvowK_l5-fCZMdeX3rg.woff2) format("woff2");
        unicode-range:
          U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2122,
          U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
      }

      @font-face {
        font-family: "Merriweather";
        font-style: normal;
        font-weight: 700;
        font-display: swap;
        src: url(/assets/fonts/u-4n0qyriQwlOrhSvowK_l52xwNZWMf6hPvhPQ.woff2) format("woff2");
        unicode-range:
          U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2122,
          U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
      }
    </style>

    {#- Favicon -#}
    <link rel="icon" type="image/svg+xml" href="/assets/images/favicon.svg" />

    {#- Page title, meta tags, and other head elements -#} {%- include "partials/meta.njk" -%}
  </head>
  <body>
    {#- Skip to content link -#}
    <a class="visually-hidden" href="#main">Skip to content</a>

    {#- Site header with navigation -#}
    <header class="content">
      <div class="header-container breakout">
        <img src="" alt="" class="logo" />
        {%- include "partials/navigation.njk" -%}
      </div>
    </header>

    {#- Main content -#}
    <main id="main" class="content" tabindex="-1" data-pagefind-body>
      <header id="page-header">
        <h1>{#- Page Title -#} {{ title | safe }}</h1>
      </header>
      {#- Content from the page or post -#} {{ content | safe }}
    </main>

    {#- Site footer with copyright -#}
    <footer id="footer" class="content">
      <small class="breakout">{%- year -%} &copy; {{ site.name }}</small>
    </footer>

    {#- Script bundle -#}
    <script src="{{ '/assets/scripts/main.js' | url }}"></script>
  </body>
</html>
